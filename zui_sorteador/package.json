{
  "name": "zui_sorteador",
  "version": "0.0.1",
  "description": "An SAP Fiori application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "dependencies": {
    "canvas-confetti": "^1.9.3",
    "js-confetti": "^0.13.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.14.7",
    "@sap-ux/eslint-plugin-fiori-tools": "^0.4.0",
    "@sap/eslint-plugin-ui5-jsdocs": "2.0.5",
    "@sap/ux-ui5-tooling": "1",
    "@sapui5/ts-types": "~1.139.0",
    "@ui5/cli": "^3.0.0",
    "eslint": "8.57.0",
    "eslint-plugin-fiori-custom": "2.6.7",
    "ui5-tooling-modules": "^3.30.2"
  },
  "ui5": {
    "dependencies": [
      "ui5-tooling-modules"
    ]
  },
  "scripts": {
    "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#zuisorteador-display\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#zuisorteador-display\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "lint": "eslint ./",
    "deploy": "fiori verify",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "start-variants-management": "fiori run --open \"preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
    "unit-tests": "fiori run --open 'test/unit/unitTests.qunit.html'",
    "int-tests": "fiori run --open 'test/integration/opaTests.qunit.html'"
  },
  "sapuxLayer": "CUSTOMER_BASE"
}
